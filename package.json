{
  "name": "dezlymacauley-dot-com",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev --open --port 6969",
    "build": "vite build",
    "preview": "vite preview --open --port 6969",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "rm -rf .svelte-kit .deno-deploy node_modules",
    "setup": "mise trust && deno task clean && deno install && deno task prepare && deno task build"
  },
  "devDependencies": {
    "@deno/svelte-adapter": "0.1.0",
    "@sveltejs/kit": "2.49.3",
    "@sveltejs/vite-plugin-svelte": "6.2.3",
    "@tailwindcss/language-server": "0.14.29",
    "@tailwindcss/vite": "4.1.18",
    "svelte": "5.46.1",
    "svelte-check": "4.3.5",
    "svelte-language-server": "0.17.23",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "typescript-language-server": "5.1.3",
    "vite": "7.3.1"
  }
}
